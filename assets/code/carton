      5.008007 => '2005-05-30',
      5.009003 => '2006-01-28',
      5.008008 => '2006-01-31',
      5.009004 => '2006-08-15',
      5.009005 => '2007-07-07',
      5.010000 => '2007-12-18',
      5.008009 => '2008-12-14',
      5.010001 => '2009-08-22',
      5.011000 => '2009-10-02',
      5.011001 => '2009-10-20',
      5.011002 => '2009-11-20',
      5.011003 => '2009-12-20',
      5.011004 => '2010-01-20',
      5.011005 => '2010-02-20',
      5.012000 => '2010-04-12',
      5.013000 => '2010-04-20',
      5.012001 => '2010-05-16',
      5.013001 => '2010-05-20',
      5.013002 => '2010-06-22',
      5.013003 => '2010-07-20',
      5.013004 => '2010-08-20',
      5.012002 => '2010-09-06',
